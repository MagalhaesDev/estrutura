// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Inventorys {
    id String @id @default(uuid())
    department String
    description String
    grup String
    marca String
    model Decimal
    quantity Decimal
    state String
    unit String
    value_un String
    date_buy DateTime?
    date_end DateTime?
}
